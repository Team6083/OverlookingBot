{
  "name": "overlookingbot",
  "version": "1.0.0",
  "description": "a slack bot for Team 6083 Overlooking",
  "main": "src/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc --reporter=text-lcov mocha test/index.js",
    "dev": "cross-env NODE_ENV=development babel-node src/index.js",
    "build": "rm -rf ./dist/ && babel src --out-dir dist/ --copy-files",
    "prod": "cross-env NODE_ENV=production node dist/index.js",
    "lint": "eslint src",
    "lintfix": "eslint src --fix",
    "report-coverage": "codecov"
  },
  "author": "Team6083",
  "license": "MIT",
  "dependencies": {
    "@slack/client": "^4.8.0",
    "babel-cli": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "config": "^2.0.1",
    "csv-write-stream": "^2.0.0",
    "express": "^4.16.4",
    "mkdirp": "^0.5.1",
    "morgan": "^1.9.1",
    "tracer": "^0.9.2"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^5.1.0",
    "codecov": "^3.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.8.0",
    "eslint-config-google": "^0.11.0",
    "eslint-plugin-react": "^7.11.1",
    "gulp-istanbul": "^1.1.3",
    "isparta": "^4.1.0",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "pre-commit": "^1.2.2"
  }
}
